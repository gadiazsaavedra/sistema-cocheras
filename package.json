{
  "name": "sistema-cocheras-backend",
  "version": "1.0.0",
  "description": "Sistema de gestión de cocheras con backup automático",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build handled by Netlify'",
    "backup": "node backup-system.js backup",
    "backup-restore": "node backup-system.js restore",
    "backup-list": "node backup-system.js list",
    "backup-scheduler": "node backup-scheduler.js start",
    "backup-manual": "node backup-scheduler.js manual",
    "backup-status": "node backup-scheduler.js status",
    "migrate": "node migrations/run-migrations.js",
    "setup-empleados": "node setup-empleados.js",
    "deploy": "npm run build && npm run migrate"
  },
  "dependencies": {
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "cocheras",
    "firebase",
    "backup",
    "nodejs"
  ],
  "author": "gadiazsaavedra",
  "license": "ISC"
}
